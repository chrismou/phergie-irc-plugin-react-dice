{
    "name": "chrismou/phergie-irc-plugin-react-dice",
    "description": "Phergie plugin for returning randomly generated numbers in response to dice rolling requests",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "homepage": "http://github.com/chrismou/phergie-irc-plugin-react-dice",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Chris Chrisostomou",
            "email": "dev@mou.me",
            "homepage": "https://mou.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@mou.me",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/chrismou/phergie-irc-plugin-react-dice/issues",
        "source": "https://github.com/chrismou/phergie-irc-plugin-react-dice"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "<3.0",
        "phergie/phergie-irc-plugin-react-command": "~2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phake/phake": "2.0.0-beta2",
        "squizlabs/php_codesniffer": "*",
        "vectorface/dunit": "^2.1",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\Phergie\\Plugin\\Dice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Phergie\\Tests\\Plugin\\Dice\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
